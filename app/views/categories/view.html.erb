<style>
	td{
		padding: 1%;
	}
</style>
<div class="table-responsive">
	<table class="table-striped">
		<tbody>
			<tr>
			<% i = 1 %>
			<% @j['products'].each do |val| %>
			<% name = val['name'].tr('/////', '') %>
			<td><a href='/products/<%= val['product_code'] %>/<%= name %>'><%= val['name'] %></a></td>
			<% if i == 0 %>
			<% elsif i % 5 == 0 %>
				</tr>
				<tr>
			<% end %>
			<% i += 1 %>
			<% end %>
		</tbody>
	</table>
</div>